{% load media %}
<div><a href='{% url tamNuovaCorsaId viaggio.id %}'>{{viaggio.da}}-{{viaggio.a}}</a></div>
{% include "corse/cella_ora_e_percorso.inc.html" %}
<div>
	{{ viaggio.numero_passeggeri }} pax
	{% if viaggio.esclusivo%}
		<img width="12px" height="12px" src="{% media_url "t.ico" %}" alt="taxi" />
	{% else %}
		<img width="12px" height="12px" src="{% media_url "c.ico" %}" alt="collettivo" />
	{% endif %}
</div>
